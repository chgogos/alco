    "version": "0.2.0",
    "configurations": [
        {
            "name": "g++.exe - Build and debug greedy_coloring (MINGW+WIN10)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\app\\src\\greedy_coloring.exe",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:\\TDM-GCC-64\\bin\\gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: g++.exe build active file"
        }
    ]
}